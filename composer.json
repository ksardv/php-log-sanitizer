{
    "name": "ksardv/php-log-sanitizer",
    "description": "A PHP package to hide or mask sensitive data in logs.",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "log",
        "sanitizer",
        "mask",
        "masking",
        "privacy",
        "gdpr",
        "pci",
        "security",
        "json",
        "xml",
        "text",
        "laravel",
        "symfony",
        "php"
    ],
    "autoload": {
        "psr-4": {
            "Ksardv\\PhpLogSanitizer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ksardv\\PhpLogSanitizer\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^12.1"
    },
    "scripts": {
        "test": "phpunit --colors=always --testdox"
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}